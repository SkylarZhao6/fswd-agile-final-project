<div class="container" style="margin-top: 30px;">
	<form class="form-inline col-md-6" id="searchForm">
		<div class="form-group" style="margin-top: 15px;">
			<label for="inputText">Enter User id: </label>
			<input
				type="text"
				name="id"
				class="form-control mx-sm-3"
				aria-describedby="passwordHelpInline"
				value=""
			/>
		</div>
		<div class="form-group" style="margin-top: 15px;">
			<label for="inputText">Enter the name: </label>
			<input
				type="text"
				name="name"
				class="form-control mx-sm-3"
				aria-describedby="passwordHelpInline"
				value=""
			/>
		</div>
		<div class="form-group" style="margin-top: 15px;">
			<label for="inputText">Enter the email: </label>
			<input
				type="text"
				name="email"
				class="form-control mx-sm-3"
				aria-describedby="passwordHelpInline"
				value=""
			/>
		</div>
		<div class="form-group" style="margin-top: 15px;">
			<label for="inputText">Enter the food you like: </label>
			<input
				type="text"
				name="food"
				class="form-control mx-sm-3"
				aria-describedby="passwordHelpInline"
				value=""
			/>
		</div>
		<div class="form-group" style="margin-top: 15px;">
			<label for="inputText">Enter your allergies: </label>
			<input
				type="text"
				name="allergies"
				class="form-control mx-sm-3"
				aria-describedby="passwordHelpInline"
				value=""
			/>
		</div>
		<button
			type="submit"
			id="submit"
			class="btn btn-outline-primary"
			style="margin-top: 15px;"
		>
			Submit user info
		</button>
	</form>
</div>
