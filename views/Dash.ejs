<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./parts/head'); -%>
		<title>Dashboard</title>
	</head>

	<body style="margin: 0;">
		<section class="hero is-primary is-fullheight is-bold">
			<div class="hero-body">
				<div class="container">
					<h1 class="title">
						Dashboard
					</h1>
					<%if (locals.msg != '') { %>
					<div class="notification is-danger">
						<h1>
							<%=msg %>
						</h1>
					</div>
					<% } else { %> <% } %>
					<div class="buttons are-medium">
						<a
							href="/login_and_signup"
							class="button is-rounded has-text-black	"
						>
							<span>Login Page</span>
						</a>
					</div>
					<div id="edamam-badge" data-color="white"></div>
				</div>
			</div>
		</section>
	</body>
</html>
